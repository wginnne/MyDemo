#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/a_study/front_end_pratice/demo_pratice/tutorial-demo/demo/node_modules/.pnpm/@rsbuild+core@1.6.12-canary-20251204065915/node_modules/@rsbuild/core/bin/node_modules:/mnt/d/a_study/front_end_pratice/demo_pratice/tutorial-demo/demo/node_modules/.pnpm/@rsbuild+core@1.6.12-canary-20251204065915/node_modules/@rsbuild/core/node_modules:/mnt/d/a_study/front_end_pratice/demo_pratice/tutorial-demo/demo/node_modules/.pnpm/@rsbuild+core@1.6.12-canary-20251204065915/node_modules/@rsbuild/node_modules:/mnt/d/a_study/front_end_pratice/demo_pratice/tutorial-demo/demo/node_modules/.pnpm/@rsbuild+core@1.6.12-canary-20251204065915/node_modules:/mnt/d/a_study/front_end_pratice/demo_pratice/tutorial-demo/demo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/a_study/front_end_pratice/demo_pratice/tutorial-demo/demo/node_modules/.pnpm/@rsbuild+core@1.6.12-canary-20251204065915/node_modules/@rsbuild/core/bin/node_modules:/mnt/d/a_study/front_end_pratice/demo_pratice/tutorial-demo/demo/node_modules/.pnpm/@rsbuild+core@1.6.12-canary-20251204065915/node_modules/@rsbuild/core/node_modules:/mnt/d/a_study/front_end_pratice/demo_pratice/tutorial-demo/demo/node_modules/.pnpm/@rsbuild+core@1.6.12-canary-20251204065915/node_modules/@rsbuild/node_modules:/mnt/d/a_study/front_end_pratice/demo_pratice/tutorial-demo/demo/node_modules/.pnpm/@rsbuild+core@1.6.12-canary-20251204065915/node_modules:/mnt/d/a_study/front_end_pratice/demo_pratice/tutorial-demo/demo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@rsbuild+core@1.6.12-canary-20251204065915/node_modules/@rsbuild/core/bin/rsbuild.js" "$@"
else
  exec node  "$basedir/../../../../../../@rsbuild+core@1.6.12-canary-20251204065915/node_modules/@rsbuild/core/bin/rsbuild.js" "$@"
fi
