#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/a_study/front_end_pratice/demo_pratice/tutorial-demo/demo/node_modules/.pnpm/@rstest+core@0.7.4_jsdom@27.3.0/node_modules/@rstest/core/bin/node_modules:/mnt/d/a_study/front_end_pratice/demo_pratice/tutorial-demo/demo/node_modules/.pnpm/@rstest+core@0.7.4_jsdom@27.3.0/node_modules/@rstest/core/node_modules:/mnt/d/a_study/front_end_pratice/demo_pratice/tutorial-demo/demo/node_modules/.pnpm/@rstest+core@0.7.4_jsdom@27.3.0/node_modules/@rstest/node_modules:/mnt/d/a_study/front_end_pratice/demo_pratice/tutorial-demo/demo/node_modules/.pnpm/@rstest+core@0.7.4_jsdom@27.3.0/node_modules:/mnt/d/a_study/front_end_pratice/demo_pratice/tutorial-demo/demo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/a_study/front_end_pratice/demo_pratice/tutorial-demo/demo/node_modules/.pnpm/@rstest+core@0.7.4_jsdom@27.3.0/node_modules/@rstest/core/bin/node_modules:/mnt/d/a_study/front_end_pratice/demo_pratice/tutorial-demo/demo/node_modules/.pnpm/@rstest+core@0.7.4_jsdom@27.3.0/node_modules/@rstest/core/node_modules:/mnt/d/a_study/front_end_pratice/demo_pratice/tutorial-demo/demo/node_modules/.pnpm/@rstest+core@0.7.4_jsdom@27.3.0/node_modules/@rstest/node_modules:/mnt/d/a_study/front_end_pratice/demo_pratice/tutorial-demo/demo/node_modules/.pnpm/@rstest+core@0.7.4_jsdom@27.3.0/node_modules:/mnt/d/a_study/front_end_pratice/demo_pratice/tutorial-demo/demo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/rstest.js" "$@"
else
  exec node  "$basedir/../../bin/rstest.js" "$@"
fi
